{
  "name": "knot-lib/command",
  "description": "Command system for kNot Framework.",
  "keywords": ["PHP","Command","kNot Framework"],
  "homepage": "https://github.com/knot-lib/command",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-framework.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "stk2k/file-system": "^0.1",
    "stk2k/string": "^0.1",
    "knot-lib/kernel": "~0.8",
    "knot-lib/services": "~0.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.15",
    "php-coveralls/php-coveralls": "^2.0",
    "knot-lib/di": "~0.2",
    "knot-lib/support": "~0.5"
  },
  "autoload": {
    "psr-4": {
      "knotlib\\command\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "knotlib\\command\\test\\": "test/",
      "knotlib\\command\\test\\classes\\": "test/classes/"
    }
  }
}